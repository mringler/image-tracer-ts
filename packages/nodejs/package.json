  "repository": {
    "type": "git",
    "url": "git@github.com:mringler/image-tracer-ts.git"
  },
  "bugs": {
    "url": "https://github.com/mringler/image-tracer-ts/issues"
  },
  "homepage": "https://github.com/mringler/image-tracer-ts"
